      if (ajax.prefetch_next_page) {
        var pageSize = carousel.getAttr(ATTR_PAGE_SIZE);
        var size = pageSize * 2;
        if (carousel.getAttr(ATTR_SHOW_PARTIAL_NEXT)) {
          size++;
        }
